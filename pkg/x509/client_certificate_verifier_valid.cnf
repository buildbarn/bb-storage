[ req ]
default_bits       = 4096
distinguished_name = distinguished_name
x509_extensions = extensions
prompt = no

[ distinguished_name ]
commonName                 = a.example.com

[ extensions ]
extendedKeyUsage =  serverAuth, clientAuth
subjectAltName = @alt_names

[ alt_names ]
DNS.1   = a.example.com
email.1 = me@example.com
URI.1 = uri:example:a
